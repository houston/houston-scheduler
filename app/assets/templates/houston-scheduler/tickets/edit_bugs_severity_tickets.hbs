{{#tickets}}
<tr class="ticket {{#if saved}}saved{{/if}}" id="ticket_{{id}}" rel="ticket" data-number="{{number}}">
  <td class="task-severity">{{severity}}</td>
  <td class="task-seriousness">{{seriousness}}</td>
  <td class="task-likelihood">{{likelihood}}</td>
  <td class="task-clumsiness">{{clumsiness}}</td>
  <td class="ticket-type ticket-type-{{type}}"></td>
  <td class="ticket-reporter">
    {{#if reporter}}
      {{{avatar reporter/email 24 ''}}}
    {{/if}}
  </td>
  <td class="ticket-summary">{{summary}}
    {{{formatPrerequisites prerequisites}}}
  </td>
  <td class="ticket-number">#{{number}}</td>
  <td class="ticket-opened">{{{formatDateWithYear openedAt}}}</td>
</tr>
{{/tickets}}
