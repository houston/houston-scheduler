<h3>
  Discussion Needed
</h3>

{{#if tickets}}
<table class="table table-sortable table-unable-to-estimate-tickets">
  <thead>
    <tr>
      <th class="ticket-effort sort-desc">Summary</th>
      <td></td>
    </tr>
  </thead>
  <tbody id="tickets">
    {{#tickets}}
      <tr class="ticket" data-ticket-id="{{id}}">
        <td class="ticket-summary">
          {{summary}}
          <a href="{{ticketUrl}}" target="_blank" rel="tooltip" data-tooltip-placement="right" data-original-title="View in {{ticketSystem}}">[#{{number}}]</a>
          {{#tags}}
            <span class="ticket-tag" style="background-color: #{{color}}">{{name}}</span>
          {{/tags}}
          <a href="/tickets/{{id}}.json" class="ticket-details" tabindex="-1">more</a>
        </td>
        <td class="ticket-command">
          {{#if unableToSetEstimatedEffort}}{{#if ../../canEstimate}}
            <button tabindex="-1" class="btn btn-success btn-can-estimate">Can Estimate!</button>
          {{/if}}{{/if}}
          {{#if unableToSetPriority}}{{#if ../../canPrioritize}}
            <button tabindex="-1" class="btn btn-success btn-can-prioritize">Can Prioritize!</button>
          {{/if}}{{/if}}
        </td>
      </tr>
    {{/tickets}}
  </tbody>
</table>
{{else}}
<div class="alert alert-success">There are no tickets that the Develpment Team is unable to estimate.</div>
{{/if}}